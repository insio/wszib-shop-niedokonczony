@model List<Shop.Core.Domain.Product>


<div class="row">
    <div class="col-lg-12">
        <h1>Products</h1>
       
        <div class="list-group">
           
            @foreach (var product in Model) {
            <a href="#" title="Add To Cart" class="list-group-item list-group-item-action">@product.Name [@product.Price PLN] </a>
            }
        </div>


    </div>
</div>
